    </nav>






    <!-- partial -->
    <div class="container-fluid page-body-wrapper">
      <!-- partial:partials/_settings-panel.html -->
      <div class="theme-setting-wrapper">
        <div id="settings-trigger"><i class="ti-settings"></i></div>
        <div id="theme-settings" class="settings-panel">
          <i class="settings-close ti-close"></i>
          <p class="settings-heading">SIDEBAR SKINS</p>
          <div class="sidebar-bg-options selected" id="sidebar-light-theme"><div class="img-ss rounded-circle bg-light border mr-3"></div>Light</div>
          <div class="sidebar-bg-options" id="sidebar-dark-theme"><div class="img-ss rounded-circle bg-dark border mr-3"></div>Dark</div>
          <p class="settings-heading mt-2">HEADER SKINS</p>
          <div class="color-tiles mx-0 px-4">
            <div class="tiles success"></div>
            <div class="tiles warning"></div>
            <div class="tiles danger"></div>
            <div class="tiles info"></div>
            <div class="tiles dark"></div>
            <div class="tiles default"></div>
          </div>
        </div>
      </div>



      <div id="right-sidebar" class="settings-panel">
        <i class="settings-close ti-close"></i>


        {{#each loginuser}}
        <ul class="nav nav-tabs border-top" id="setting-panel" role="tablist">



          {{#if isAdmin}}

           <li class="nav-item">
            <a class="nav-link" id="chats-tab" data-toggle="tab" href="#Admin-section" role="tab" aria-controls="chats-section">Admin Notifications</a>
          </li>


          <li class="nav-item">
            <a class="nav-link" id="chats-tab" data-toggle="tab" href="#Sub-admin-section" role="tab" aria-controls="chats-section">Sub admin/ Doc Notifications</a>
          </li>

        </ul>

        {{else if isSuperadmin}}

         <ul class="nav nav-tabs border-top" id="setting-panel" role="tablist">
           <li class="nav-item">
            <a class="nav-link active" id="todo-tab" data-toggle="tab" href="#todo-section" role="tab" aria-controls="todo-section" aria-expanded="true">Super Admin Notifications</a>
          </li>

           <li class="nav-item">
            <a class="nav-link" id="chats-tab" data-toggle="tab" href="#Admin-section" role="tab" aria-controls="chats-section">Admin Notifications</a>
          </li>


          <li class="nav-item">
            <a class="nav-link" id="chats-tab" data-toggle="tab" href="#Sub-admin-section" role="tab" aria-controls="chats-section">Sub admin/ Doc Notifications</a>
          </li>

        </ul>

         {{else}}

          <ul class="nav nav-tabs border-top" id="setting-panel" role="tablist">
        
          <li class="nav-item">
            <a class="nav-link" id="chats-tab" data-toggle="tab" href="#Sub-admin-section" role="tab" aria-controls="chats-section">Sub admin/ Doc Notifications</a>
          </li>

        </ul>

          {{/if}}

      {{/each}}


        <div class="tab-content" id="setting-content">
          <div class="tab-pane fade show active scroll-wrapper" id="todo-section" role="tabpanel" aria-labelledby="todo-section">
            <div class="add-items d-flex px-3 mb-0">
            
            </div>
            <div class="list-wrapper px-3">
              <ul class="d-flex flex-column-reverse todo-list">
                 {{#each patientdata}}
                <li class="completed"> 
                  <div class="preview-item-content">
                  <h6 class="preview-subject font-weight-normal">New patient registration by {{this.DocId}}</h6>
                  <p class="font-weight-light small-text mb-0 text-muted">
                    {{this.createdAt}}
                  </p>
                </div>
                  <i class="remove ti-close"></i>
                </li>
                  {{/each}}



                   {{#each docData}}
                <li class="completed"> 
                  <div class="preview-item-content">
                  <h6 class="preview-subject font-weight-normal">New Doctor / Sub admin registration by {{this.adminid}}</h6>
                  <p class="font-weight-light small-text mb-0 text-muted">
                    {{this.createdAt}}
                  </p>
                </div>
                  <i class="remove ti-close"></i>
                </li>
                  {{/each}}



                  {{#each admin}}
                <li class="completed"> 
                  <div class="preview-item-content">
                  <h6 class="preview-subject font-weight-normal">New Admin {{this.username}} Registered by Super Admin </h6>
                  <p class="font-weight-light small-text mb-0 text-muted">
                    {{this.createdAt}}
                  </p>
                </div>
                  <i class="remove ti-close"></i>
                </li>
                  {{/each}}
              </ul>
            </div>
          </div> 


          <div class="tab-pane fade show active scroll-wrapper" id="Admin-section" role="tabpanel" aria-labelledby="todo-section">
            <div class="add-items d-flex px-3 mb-0">
            
            </div>
            <div class="list-wrapper px-3">
              <ul class="d-flex flex-column-reverse todo-list">
                 {{#each patientdata}}
                <li class="completed"> 
                  <div class="preview-item-content">
                  <h6 class="preview-subject font-weight-normal">New patient registration by {{this.DocId}}</h6>
                  <p class="font-weight-light small-text mb-0 text-muted">
                    {{this.createdAt}}
                  </p>
                </div>
                  <i class="remove ti-close"></i>
                </li>
                  {{/each}}



                   {{#each docData}}
                <li class="completed"> 
                  <div class="preview-item-content">
                  <h6 class="preview-subject font-weight-normal">New Doctor / Sub admin registration by {{this.adminid}}</h6>
                  <p class="font-weight-light small-text mb-0 text-muted">
                    {{this.createdAt}}
                  </p>
                </div>
                  <i class="remove ti-close"></i>
                </li>
                  {{/each}}



              
              </ul>
            </div>
          </div>


           <div class="tab-pane fade show active scroll-wrapper" id="Sub-admin-section" role="tabpanel" aria-labelledby="todo-section">
            <div class="add-items d-flex px-3 mb-0">
            
            </div>
            <div class="list-wrapper px-3">
              <ul class="d-flex flex-column-reverse todo-list">
                 {{#each patientdata}}
                <li class="completed"> 
                  <div class="preview-item-content">
                  <h6 class="preview-subject font-weight-normal">New patient registration by {{this.DocId}}</h6>
                  <p class="font-weight-light small-text mb-0 text-muted">
                    {{this.createdAt}}
                  </p>
                </div>
                  <i class="remove ti-close"></i>
                </li>
                  {{/each}}
              </ul>
            </div>
          </div>



        </div>
      </div>
      <!-- partial -->